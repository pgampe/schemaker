{
    "name": "fluidtypo3/schemaker",
    "description": "The schemaker package from FluidTYPO3",
    "type": "typo3-cms-extension",
    "version": "3.0.5",
    "require": {
        "fluidtypo3/vhs": "*"
    },
    "require-dev": {
        "typo3/cms": "6.2.7",
        "fluidtypo3/development": "*"
    },
    "repositories": [
        {
            "type": "package",
            "package": {
                "name": "typo3/cms",
                "version": "6.2.7",
                "dist": {
                    "url": "https://github.com/TYPO3/TYPO3.CMS/archive/6.2.7.zip",
                    "type": "zip"
                }
            }
        }
    ],
    "autoload": {
        "psr-4": {
            "FluidTYPO3\\Schemaker\\": "Classes/"
        }
    }
}
